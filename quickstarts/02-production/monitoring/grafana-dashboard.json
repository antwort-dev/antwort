      "title": "Request Rate",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "rate(antwort_requests_total[5m])",
          "legendFormat": "{{method}} {{path}} {{status}}"
        }
      ],
      "fieldConfig": {
        "defaults": {

        {
          "expr": "sum by (status) (increase(antwort_requests_total[1h]))",
          "legendFormat": "{{status}}"
        }
      ],
      "fieldConfig": {
        "defaults": {},
        "overrides": []
      }
    },
    {
      "title": "Total Requests",
      "type": "stat",
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 16 },
